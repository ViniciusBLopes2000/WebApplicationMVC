@model IEnumerable<WebApplication1.Models.CarroModel>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Modelo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Dono)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Placa)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ano)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Modelo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dono)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Placa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ano)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = (Model as List<WebApplication1.Models.CarroModel>).IndexOf(item) }) |
                @Html.ActionLink("Details", "Details", new { id = (Model as List<WebApplication1.Models.CarroModel>).IndexOf(item) }) |
                @Html.ActionLink("Delete", "Delete", new { id = (Model as List<WebApplication1.Models.CarroModel>).IndexOf(item) }) |
            </td>
        </tr>
    }

</table>
